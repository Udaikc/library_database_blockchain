<!-- index.html -->
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Add Book</title>
</head>

<body>
    <h1>Add Book</h1>
    <form id="addBookForm" action="/users/addbooks" method="POST">
        <label for="title">Title:</label><br>
        <input type="text" id="title" name="title" required><br><br>

        <label for="author_name">Author:</label><br>
        <input type="text" id="author_name" name="author_name" required><br><br>

        <label for="published_year">Published Year:</label><br>
        <input type="number" id="published_year" name="published_year" required><br><br>


        <label for="total_number">total Number:</label><br>
        <input type="number" id="total_number" name="total_number" required><br><br>

        <label for="book_url">Book URL:</label><br>
        <input type="url" id="book_url" name="book_url" required><br><br>

        <button type="submit">Add Book</button>
    </form>
</body>

</html>